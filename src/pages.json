{
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "uni-app"
			}
		},
		{
			"path": "pages/user/index",
			"style": {
				"navigationBarTitleText": "user"
			}
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"tabBar": {
		"color": "#bfbfbf",
		"iconWidth": "20px",
		"selectedColor": "#000",
		"backgroundColor": "#fff",
		"list": [
			{
				"pagePath": "pages/index/index",
				"text": "首页",
				"iconPath": "static/shouye.png",
				"selectedIconPath": "static/shouye_selected.png"
			},
			{
				"pagePath": "pages/user/index",
				"text": "联系客服",
				"iconPath": "static/kefu.png",
				"selectedIconPath": "static/kefu_selected.png"
			}
		]
	},
	"easycom": {
		"autoscan": true,
		"custom": {
			"uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue",
			"custom-(.*)": "@/components/custom-$1/custom-$1.vue"
		}
	}
}